   <div type="Stanza" sameAs="" xml:id="Y26.11">
      <include href="/db/apps/cab_db/cab_rituals/blocks/prayers/YeH" xpointer="YeH"/>
      <include href="/db/apps/cab_db/cab_rituals/nerang/" xpointer="Y26.11NerangTransc.a"/>
